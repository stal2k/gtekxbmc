<addon id="repository.gtv" name="gTeK gTV Repo" version="1.2" provider-name="Mikey1234">
	<extension point="xbmc.addon.repository" name="gTeK gTV Add-ons">
		<info compressed="false">https://github.com/stal2k/gtekxbmc/raw/master/addons.xml</info>
		<checksum>https://github.com/stal2k/gtekxbmc/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/stal2k/gtekxbmc/raw/master/repo</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install gTV addons</summary>
		<description>Download and install addons from gTeK. Includes originals and Optimzed versions of other addons not created by gTeK. visit us online at www.gtek-memphis.com</description>
		<platform>all</platform>
	</extension>
</addon>

